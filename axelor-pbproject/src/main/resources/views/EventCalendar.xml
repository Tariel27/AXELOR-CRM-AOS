<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

    <action-view name="pbp.events.view" title="Events" model="com.axelor.apps.base.db.ICalendarEvent">
        <view type="calendar" name="pbp.events.calendar"/>
    </action-view>

    <calendar name="pbp.events.calendar" model="com.axelor.apps.base.db.ICalendarEvent" title="Events" colorBy="calendar"
              eventStart="startDateTime" eventStop="endDateTime">
        <field name="subject"/>
    </calendar>

</object-views>