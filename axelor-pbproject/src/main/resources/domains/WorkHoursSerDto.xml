<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
                http://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

    <module name="pbp" package="com.axelor.apps.pbproject.db"/>

    <entity name="WorkHoursDto">
        <string name="userName" title="User Name" large="true"/>
        <string name="projectName" title="Project Name" large="true"/>
        <decimal name="totalWorkedHours" title="Total Worked Hours" precision="10" scale="2"/>
        <long name="taskCount" title="Task Count" default="0"/>
        <string name="readableTime" title="Spent Hours"/>
    </entity>

</domain-models>
