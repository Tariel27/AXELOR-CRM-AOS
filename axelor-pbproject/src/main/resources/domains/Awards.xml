<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

    <module name="pbproject" package="com.axelor.apps.pbproject.db"/>

    <entity name="Award">
        <string name="code" title="Code" unique="true"/>
        <binary name="imageAward" title="image"/>
        <string name="name" title="Name of award"/>
        <string name="description" title="Description"/>
        <extra-code>
            <![CDATA[
	   			public static final String HOURS_500_CODE = "500hours";
	   			public static final String HOURS_1000_CODE = "1000hours";
	   			public static final String TASKS_30_CODE = "30tasks";
	   		]]>
        </extra-code>
    </entity>
</domain-models>
