<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

    <module name="project" package="com.axelor.apps.pbproject.db"/>

    <entity name="UserHoursManagement">

        <!--Связь с проектами-->
        <many-to-many name="project" ref="com.axelor.apps.project.db.Project" title="Project"/>
        <!--Связь со всем сотрудниками-->
        <many-to-many name="membersUserSet" ref="com.axelor.auth.db.User" title="Membres"/>
        <!--Связь с ДТО чтобы отображать грид-->
        <one-to-many name="workHours" ref="com.axelor.apps.pbproject.db.WorkHoursDto" title="Work hours"/>
        <!--Дата начала задачи-->
        <date name="fromDate" title="From Date"/>
        <!--Дата окончания задачи-->
        <date name="toDate" title="To Date"/>
     </entity>

</domain-models>
